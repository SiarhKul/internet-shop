{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [],
  "parserOptions": {
    "sourceType": "module"
  },
  "plugins": [
    "import"
  ],
  "rules": {
    "indent": "off",
    "no-multi-spaces": "error",
    "no-multiple-empty-lines": "error",
    "no-unused-vars": "off",
    "no-use-before-define": "error",
    "no-var": "error",
    "prefer-const": "error",
    "semi": "error",
    "space-in-parens": "error",
    "unicorn/prevent-abbreviations": "off"
  }
}